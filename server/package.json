{
    "name": "walrus-backend",
    "version": "1.0.0",
    "scripts": {
        "dev": "next dev",
        "build": "prisma generate && node scripts/check-and-resolve-migration.js && (npm run migrate:deploy || echo 'Migration failed, continuing...') && next build",
        "start": "next start",
        "postinstall": "prisma generate",
        "migrate:deploy": "node scripts/run-prisma.js migrate deploy",
        "migrate:dev": "node scripts/run-prisma.js migrate dev",
        "migrate:status": "node scripts/run-prisma.js migrate status",
        "migrate:baseline": "node scripts/baseline-migrations.js",
        "db:push": "node scripts/run-prisma.js db push",
        "prisma:generate": "prisma generate",
        "prisma:studio": "node scripts/run-prisma.js studio"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.969.0",
        "@aws-sdk/credential-providers": "^3.969.0",
        "@aws-sdk/node-http-handler": "^3.370.0",
        "@mysten/sui": "^1.40.0",
        "@mysten/walrus": "^0.7.3",
        "@noble/hashes": "^2.0.1",
        "@prisma/client": "^6.19.0",
        "bcryptjs": "^3.0.3",
        "dotenv": "^16.6.1",
        "next": "^14.2.0",
        "pg": "^8.11.3",
        "react": "^18.3.0",
        "react-dom": "^18.3.0",
        "stripe": "^20.1.2",
        "zxcvbn": "^4.4.2"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/node": "^24.7.2",
        "@types/react": "^19.2.2",
        "prisma": "^6.19.0",
        "tsx": "^4.21.0",
        "typescript": "^5.4.0"
    }
}
